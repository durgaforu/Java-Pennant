package com.car;

import com.wheel.MRFWheel;
import com.wheel.Wheel;

/*
 *  --> IS-A (Inheritance )
 * 
 *  --> HAS-A ( have reference of that )
 * 
 */

public class Car {

	Wheel wheel; // HAS-A

	public void move(Wheel wheel) {
		// Rotate wheel
		wheel.rotate();
		// wheel.iternalbehav();

		System.out.println("Car Moving");
	}

}
