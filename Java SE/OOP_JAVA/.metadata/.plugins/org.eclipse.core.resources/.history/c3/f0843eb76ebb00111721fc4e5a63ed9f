package com;

class Outer {

	// 1. State
	int out_ins;
	static int out_sta;

	// 2. Cons
	// 3. Methods
	public void out_ins_methos() {

	}

	public void out_sta_methos() {

	}

	// 4. Inner Class
	public class Inner {

	}

}

public class InnerClassDemo {

	public static void main(String[] args) {

		Outer.Inner i = new Outer().new Inner();

	}

}
