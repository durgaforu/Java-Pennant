package com.emp;

class A extends Object {

	public A() {
		super();
		System.out.println("I am A");
	}

}

class B extends A {

	public B() {
		super();
		System.out.println("I am B");
	}
}

class C extends B {

	public C() {
		super();
		System.out.println("I am C");
	}
}

public class InheritanceSyntax {

	public static void main(String[] args) {

		C cobj = new C();
	}

}
