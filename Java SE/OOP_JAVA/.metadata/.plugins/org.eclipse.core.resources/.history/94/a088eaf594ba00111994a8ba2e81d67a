package com;

class Person {

	public void doIncrement() {
		int countVar = 0; // Local Var
		countVar++;
		System.out.println("count:" + countVar);
	}

}

public class VarScopeDemo {

	public static void main(String[] args) {

		Person person1 = new Person();
		Person person2 = new Person();

		person1.doIncrement();
		person2.doIncrement();

	}

}
