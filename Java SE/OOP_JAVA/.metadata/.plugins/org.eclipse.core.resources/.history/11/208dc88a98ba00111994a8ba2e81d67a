package com;

/*
 *  default init values for variables
 *  --------------------------------
 *  
 *  
 *  1. Integral  --> 0
 *  2. Real      --> 0.0
 *  3. char      --> \u0000
 *  4. boolean   --> false
 *  
 *  5. reference  --> null
 * 
 */

class Abc {

	static int staVar;

	public void showValue() {
		System.out.println(staVar);
	}

}

public class VarInitDemo {

	public static void main(String[] args) {

		Abc abc;

	}

}
